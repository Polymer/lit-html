{
  "name": "@lit-labs/cli",
  "description": "Tooling for Lit development",
  "version": "0.0.0",
  "author": "Google, LLC",
  "bin": {
    "lit": "./bin/run"
  },
  "bugs": "https://github.com/Polymer/lit-html/issues",
  "dependencies": {
    "@lit/localize-tools": "^0.1.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "stdout-stderr": "^0.1.13",
    "typescript": "^3.9.9",
    "uvu": "^0.5.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Polymer/lit-html",
  "keywords": [
    "oclif"
  ],
  "license": "BSD 3-Clause",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "lit",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Polymer/lit-html",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "uvu test '.*_test\\.js$'",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
